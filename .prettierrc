{
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "singleAttributePerLine": false,
    "bracketSameLine": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 120,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 3,
    "trailingComma": "es5",
    "useTabs": true,
    "parser": "typescript"
}